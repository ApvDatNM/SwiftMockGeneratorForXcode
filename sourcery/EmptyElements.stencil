{% for type in types.types where type.name != "Declarations" and type.name != "NamedElement" %}
extension {{type.name}}Impl {
  static func empty{{type.name}}() -> {{type.name}} {
    return {{type.name}}Impl({% if type.implements.LeafNode or type.name == "LeafNode" %}text: ""{% else %}children: []{% endif %})
  }
}

{% endfor %}
