#!/usr/bin/env bash -e

rm -rf build || true
xcrun xcodebuild -workspace MockGenerator.xcworkspace -derivedDataPath build -scheme SwiftStructureInterface test | xcpretty -t
xcrun xcodebuild -workspace MockGenerator.xcworkspace -derivedDataPath build -scheme MockGenerator test | xcpretty -t
xcrun xcodebuild -workspace MockGenerator.xcworkspace -derivedDataPath build -scheme MockGeneratorApp test | xcpretty -t
